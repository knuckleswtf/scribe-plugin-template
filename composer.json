{
  "name": "me/my-scribe-plugin",
  "license": "MIT",
  "description": "Custom Scribe plugin to add stuff.",
  "keywords": [
    "API",
    "documentation",
    "laravel",
    "scribe",
    "scribe strategy"
  ],
  "homepage": "http://github.com/me/my-scribe-plugin",
  "authors": [
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "knuckleswtf/scribe": "^3.0"
  },
  "require-dev": {
    "dms/phpunit-arraysubset-asserts": "^0.1.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Me\\MyScribePlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Me\\MyScribePlugin\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --stop-on-failure",
    "test-ci": "phpunit"
  }
}
